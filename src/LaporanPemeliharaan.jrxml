	<queryString language="SQL">
		<![CDATA[SELECT aset.nama_aset,
	pemeliharaan.tanggal_mulai,
	pemeliharaan.tanggal_selesai,
	pemeliharaan.deskripsi_masalah,
	pemeliharaan.total_biaya
FROM pemeliharaan
	INNER JOIN aset ON 
	 pemeliharaan.kode_aset_unik = aset.kode_aset_unik
